  <% @page_heading = _('Edit User') %>
  <% content_for('tasks') do %>
  <%= tab _("New User"), :controller=>"users", :action=>"new"%>
  <%= task_overview %>
  <% end%>

  <% form_tag :action=>"edit", :id => @user.id do %>

  <h3><%= _("Editing user")%></h3>
  <div class="form">
    <%= render_partial "form" %>
  </div>

  <div id="operations">
    <%= save %> or <%= cancel %>
  </div>

  <% end %>

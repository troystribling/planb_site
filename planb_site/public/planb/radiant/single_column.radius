<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.1//EN"
  "http://www.w3.org/TR/xhtml11/DTD/xhtml11.dtd">
<html>
  <head>
    <title><r:title /></title>
    <link href="/rss/" rel="alternate" title="RSS" type="application/rss+xml" />
    <link rel="stylesheet" type="text/css" href="/planb/css/single_column.css" />
    <link rel="stylesheet" type="text/css" href="/planb/css/site.css" />
    <link rel="shortcut icon" href="/favicon.ico" />	  
  </head>
  <body>
    <div id="navigation-wrapper">
      <div id="navigation">
        <r:content part="navigation" inherit="true" />
      </div>
    </div>
    <div id="banner-wrapper">
      <div id="banner">
        <r:content part="banner" inherit="true" />
      </div>
    </div>
    <div id="page">
      <div class="section-wrapper">
        <div class="section">
          <r:content />
        </div>
      </div>
      <r:snippet name="footer" />
    </div>
  </body>
</html>
